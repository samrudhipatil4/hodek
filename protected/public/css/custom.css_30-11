    cursor: pointer;
    overflow: hidden;
}
.multiselect-box .selected-label .icon-arrow{
    position: absolute;
    top: 0;
    right: 0px;
    height: 3rem;
    padding: 8px 5px;
    pointer-events: none;
    color: #34B0E6;
}

.multiselect-box .selected-item {
    background-color: #f1f1f1;
    border-radius: 5px;
    display: inline;
    font-size: 14px;
    height: 3rem;
    line-height: 3rem;
    margin-right: 5px;
    padding: 5px;
}
.multiselect-box .selected-label + ul{
    display: none;
    margin:0px;
    box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
