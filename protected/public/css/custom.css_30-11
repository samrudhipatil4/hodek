    visibility: visible;
}

.multiselect-box { position: relative; margin-top: 1rem;}
.multiselect-box > label {
    left: 0;
    opacity: 1;
    position: absolute;
    top: -15px;
    z-index: 10;
}
.multiselect-box .selected-label{
    background-color: rgba(255, 255, 255, 0.9);
    border-bottom: 1px solid #dce0e3;
    border-radius: 0px;
    height: 3rem;
    padding: 5px 30px 5px 0;
    width: 100%;
