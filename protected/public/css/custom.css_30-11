
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,700,600italic,700italic,800);

/* font-family: 'Open Sans', sans-serif; */

/*Material Icons */
@font-face {
    font-family: 'Material Icons';
    font-style: normal;
    font-weight: 400;
    src: local('Material Icons'), local('MaterialIcons-Regular'), url(https://fonts.gstatic.com/s/materialicons/v7/2fcrYFNaTjcS6g4U3t-Y5ewrjPiaoEww8AihgqWRJAo.woff) format('woff');
}

.material-icons {
    font-family: 'Material Icons';
    font-weight: normal;
    font-style: normal;
    font-size: 24px;
    line-height: 1;
    letter-spacing: normal;
    text-transform: none;
    display: inline-block;
    word-wrap: normal;
    -moz-font-feature-settings: 'liga';
    -moz-osx-font-smoothing: grayscale;
}

/*------ General Classes -------*/


.mg-none{ margin: 0 !important;}
.mg-top-0{ margin-top: 0 !important;}
.mg-right-0{ margin-right: 0 !important;}
.mg-bottom-0{ margin-bottom: 0 !important;}
.mg-left-0{ margin-left: 0 !important;}
.mg-top-20{ margin-top: 20px !important;}
.mg-bottom-20{ margin-bottom: 20px !important;}
.pd-none{ padding: 0 !important;}
.pd-top-0{ padding-top: 0 !important;}
.pd-right-0{ padding-right: 0 !important;}
.pd-bottom-0{ padding-bottom: 0 !important;}
.pd-left-0{ padding-left: 0 !important;}
.pd-20{ padding: 20px !important;}
.shadow-none{ box-shadow: none !important;}
.border-none{ border: 0px !important; }
.color-skyblue{ color: #34B0E6;}
.color-darkgrey{ color: #38393B;}
.color-grey{ color: #777A7F;}

.re-initiated{ color: #248CCB;}
.accepted{ color: #15CCAE;}
.rejected{ color:#F05D6D ;}

.size-12{ font-size: 12px !important;}
.size-14{ font-size: 14px !important;}
.size-16{ font-size: 16px !important;}
.center-align{ text-align: center !important;}

/*------ General Setting -------*/
body {
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -o-flex-direction: column;
    -ms-flex-direction: column;
    font-family: 'Open Sans', sans-serif;
    font-weight: normal;
    font-size: 16px;
    background-color: #FAFAFA;
}
.main-wrapper {
    flex: 1 0 auto;
    -webkit-flex: 1 0 auto;
    -moz-flex: 1 0 auto;
    -o-flex: 1 0 auto;
    -ms-flex: 1 0 auto;
}


.main-wrapper > .wrapper, .footer-copyright > .wrapper{ padding: 0 15px;}
a, a:focus{ outline: 0 none !important;}
a:hover{  color: #38393B;}
strong{ font-weight: 600 !important;}
label{ color: #34b0e6 !important; }


/* Header styles*/
.top-navigation { background-color: #ffffff; box-shadow: none !important; height: 90px;}
.top-navigation .nav-wrapper{ height: 90px;}
.top-navigation .nav-wrapper .logo-wrapper{
    width: 18.7%;
    float: left;
    padding-top: 20px;
    padding-left: 30px;
    background-color: #ffffff;
    border-bottom: 1px solid #E5E5E5;
    height: 90px;
}

.navbar-fixed {  height: 90px;}

.top-navigation .nav-wrapper .menu-wrapper{
    width: 81.3%;
    float: left;
    padding:0 30px 0 22px;
    background-color: #248CCB;
}
.top-navigation .nav-wrapper .menu-wrapper div[class*="col"]{ padding: 0px;}

.top-navigation .nav-wrapper .menu-wrapper li a{
    line-height: 90px !important;
    text-transform: uppercase;
    font-size: 0.875em;
}
.top-navigation .nav-wrapper .menu-wrapper li a:hover, .top-navigation .nav-wrapper .menu-wrapper li.active a:hover{ color: #ffffff;}
.top-navigation .nav-wrapper .menu-wrapper li.active a{
    background-color: rgba(0, 0, 0, 0.1);
}

.top-navigation .nav-wrapper .menu-wrapper li a i{ line-height: auto !important; display: inline-block; vertical-align: middle;}
#left-nav li a i{ font-size: 1.5em;}

.top-navigation .nav-wrapper .menu-wrapper li .icon{
    background: url('../images/topnav-icons.png') no-repeat 0 0;
    width: 21px;
    height: 21px;
    display: inline-block;
}
.top-navigation .nav-wrapper .menu-wrapper li .icon.dashboard{  background-position: 2px -1px; }
.top-navigation .nav-wrapper .menu-wrapper li .icon.add{  background-position: -140px -1px; }
.top-navigation .nav-wrapper .menu-wrapper li .icon.view{  background-position: -353px -1px; width:25px; }
.top-navigation .nav-wrapper .menu-wrapper li .icon.search{  background-position: -453px -3px; }
.top-navigation .nav-wrapper .menu-wrapper li .icon.report{  background-position: -568px -1px; }
.top-navigation .nav-wrapper .menu-wrapper li .icon.scm{  background-position: -685px 0; width: 30px; }



/*----- Header styles ---------*/

.top-navigation .profile{
    position: relative;
}
.top-navigation .profile .user-icon {
    border: 1px solid #DFDFDF;
    border-radius: 100%;
    height: 40px;
    margin-right: 10px;
    vertical-align: middle;
    width: 40px;
}
.top-navigation .profile:hover .user-icon { border-color: #282828;}

.top-navigation .profile > a span{ position: relative; padding-right: 20px;}
.top-navigation .profile > a span::after{
    content: "";
    border-top:6px solid #ffffff;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-bottom: 6px solid transparent;
    height: 6px;
    width: 6px;
    position: absolute;
    right: 0;
    top:50%;
    margin-top:-1px;
}
.top-navigation .notification span {
    background-color: #ff6030;
    border-radius: 2px;
    color: #ffffff;
    line-height: normal;
    padding: 0px 5px;
    position: absolute;

    top: 23px;
}
.top-navigation .profile .dropdown{
    position: absolute;
    top: 100%;
    right: 0px;
    min-width: 190px;
    background-color:#248CCB;
    display: none;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}
.top-navigation .profile .dropdown li {
    display: block !important;
    width: 100%;
}
.top-navigation .profile .dropdown li a{color: #ffffff; display: block; line-height: 64px !important; }
.top-navigation .profile .dropdown li:last-child{ border-bottom: 0px;}
.top-navigation .profile .dropdown li a i{ display: inline-block !important; vertical-align: bottom; margin-right: 10px; font-size: 1.5em;}



/*------ Footer styles ---------*/
footer.page-footer {
    background-color: #D9DCE1;
    margin-top: 2.1rem;
    padding-top: 0px;
}
footer.page-footer .footer-copyright {
    background-color: transparent;
    color: #3C495C !important;
    height: auto;
    line-height: normal;
    overflow: hidden;
    padding: 20px 0;
}

footer.page-footer .footer-copyright .copyright{
    font-size: 13px;
    margin-top: 12px;
}

/*------ Home page style ---------*/

.page-heading > h1, .page-heading > h2{
    font-size: 16px;
    padding-bottom: 20px;
    border-bottom: 1px solid #E5E5E5;
    position: relative;
    margin-bottom: 0px;
    font-weight: normal;
}
.page-heading > h1 > span{ font-weight: 300;}
.page-heading > h2{ text-transform: uppercase;}

/* card panel */

.counter hgroup h5{ font-size: 14px; color: #6B6E72; text-transform: uppercase;}
.counter hgroup h2{ margin: 0px; font-size: 40px; font-weight: 300; color:#35B1E5;}

.counter .circle-icon{
    width: 100px; height: 100px;
    border: 4px solid #DADEE1;
    background-color: #ffffff;
    border-radius: 100%; text-align: center;
}
.counter .circle-icon > i{ font-size: 2.5rem; color: #EF5461; line-height: 90px;}

/* chart card */

.card-panel {
    background-color: #fff;
    border-radius: 5px;
    margin: 0.5rem 0 1rem;
    padding: 15px;
    border: 1px solid #DCE0E3;
}

.chart-card .card-image{
    background-color: #ffffff;
    border-style: solid;
    border-width: 1px 1px 0 1px;
    border-color: #dce0e3;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}
.chart-card .card-content{
    background-color: #EEF2F5;
    border-style: solid;
    border-width: 2px 1px 1px 1px;
    border-color:#4DB1F0 #dce0e3 #dce0e3 #dce0e3;
    padding: 10px 20px;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}
.chart-card .card-content .card-title, .chart-card .card-reveal .card-title{
    line-height: 24px;
    font-size:15px;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-weight:400;
    color: #878F9A;
}
.chart-card .card-content .card-title > i{ font-size: 28px;}
.chart-card .card-reveal .card-title > i{ font-size: 22px;}
.chart-card .card-reveal {
    background-color:#4DB1F0;
    color: #ffffff
}
.chart-card .card-reveal .card-title{ color: #ffffff;}
.chart-card .card-reveal p{ font-size: 14px;}
.legend li{ margin-bottom: 5px; font-size: 14px;}
.legend li span{
    height: 15px;
    width: 15px;
    border-radius: 2px;
    vertical-align: middle;
    display: inline-block;
    margin-right: 10px;
    border: 1px solid #ffffff;
}
.legend li span.tomato{	background-color: #E75E66;}
.legend li span.sky-blue{	background-color: #4CC0E5;}
.legend li span.grey{ background-color: #DADEE1 !important;}
.legend li span.dark-green{	background-color: #0B8CA2;}
.legend li span.green{	background-color: #5DB75D;}
.legend li span.yellow{	background-color: #FEC045;}
.legend li span.purple{	background-color: #CA6FF0;}
.legend li span.dark-yellow{	background-color: #FFAB58;}



/* Accordian tasks */
.accordian-task{
    box-shadow: none;
    border: 0px;
    margin: 0px;
}
.accordian-task .collapsible-header{
    background-color: #34B0E6;
    padding: 0 1.25rem;
    color: #ffffff;
    border-radius: 5px;
}
.accordian-task .collapsible-header.active{
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}
.accordian-task .collapsible-body{
    background-color: #ffffff;
    border-style: solid;
    border-width: 0px 1px 1px 1px;
    border-color:transparent #dce0e3 #dce0e3 #dce0e3;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}
.accordian-task .collapsible-body .toolbar{
    padding: 0.5rem 1.25em;
    background-color: #EEF2F5;
    border-bottom: 1px solid #E5E5E5;
    font-size: 0.750em;
    color: #6B6E74;
}
.accordian-task .collapsible-body .table-wrapper{
    padding: 1.25rem;
}
.accordian-task .collapsible-header .records{
    background-color: #ffffff;
    border: 1px solid #7F8289;
    color: #7F8289;
    border-radius: 20px;
    margin-top: 8px;
    text-align: right;
}
.accordian-task .collapsible-header .records{
    font-size: 12px;
    line-height: 26px;
    padding: 0 12px 0 18px;
}
.accordian-task .collapsible-header .records > i{
    padding: 0 10px;
    font-size: 14px;
    line-height: 26px;
    -webkit-transition:all 200ms ease-in-out;
    -moz-transition:all 200ms ease-in-out;
    -o-transition:all 200ms ease-in-out;
    -ms-transition:all 200ms ease-in-out;
    transition:all 200ms ease-in-out;
}
.accordian-task .collapsible-header.active .records > i{
    -webkit-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);
}

.accordian-task .collapsible-body .table-wrapper tr td a:hover{  color: #34B0E6 !important; }

.pagination-wrapper{ margin-top: 5px;}
.pagination-wrapper > ul > li{
    display: inline-block !important;
    vertical-align: middle;
    line-height: 25px;
    border-right: 1px solid #9FA6AE;
    padding: 0 10px;
}
.pagination-wrapper > ul > li:first-child{ padding-left: 0px; }
.pagination-wrapper > ul > li:last-child{ border-right: 0px;}

.pagination-wrapper .pagination{
    display: inline-block;
}
.pagination-wrapper .pagination li{ font-size:12px; vertical-align: middle; line-height: 25px; background-color: transparent; border-radius: 0px; padding: 0 2px;}
.pagination-wrapper .pagination li:first-child, .pagination-wrapper .pagination li:last-child{
    border-radius: 100%;
    /* border: 1px solid #9FA6AE; */
    font-size: 12px;
    height: 25px;
    width: 25px;
    line-height: 25px;
    padding: 0px;
}

.pagination-wrapper .pagination li i{  font-size: 16px;  }
.pagination-wrapper .pagination li a, .pagination-wrapper .pagination li a.active{ color: #6B6E74 !important;}

.pagination-wrapper .row div[class*="col"]{ padding: 0px;}
.pagination-wrapper .select-box{
    position: relative;
    width: 60px;
    display: block;
    /*margin-left: 5px;*/
}
.pagination-wrapper .select-box select{
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    border: 1px solid #9FA6AE;
    height: 25px;
    line-height: 25px;
    padding: 0 0 0 5px !important;
    border-radius: 5px;
    outline: none;
    cursor: pointer;
    opacity: 1;
}
.pagination-wrapper .select-box::after{
    font-family: "FontAwesome" !important;
    content:"\f0d7";
    height: 19px;
    width: 18px;
    display: inline-block;
    position: absolute;
    right: 3px;
    top: 3px;
    background-color:#EEF2F5;
    color: #000000;
    border: 1px solid #9FA6AE;
    border-radius: 5px;
    vertical-align: middle;
    padding: 0;
    text-align: center;
    line-height: 18px;
    pointer-events:none;

}
.pagination-wrapper > ul ul li{ display: inline-block;}

.search-form {
    float: right;
    width: 200px;
    background-color: #FFFFFF;
    border-radius: 1px solid #9FA6AE;
    padding: 5px 40px 5px 30px;
    border-radius: 50px;
    border: 1px solid #9FA6AE;
    position: relative;
    margin-right:-6px;
}

.search-form input[type="text"]{
    margin-bottom: 0px;
    height: 20px;
    border: 0px;
    font-size: 14px;
    color: #000000;
    font-style: italic;
}
.search-form input[type="text"]:focus{ border: none 0px !important; box-shadow: none !important;}
.search-form ::-webkit-input-placeholder { /* WebKit, Blink, Edge */
    color: #6B6E74 !important;
}
.search-form :-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #6B6E74 !important;
    opacity:  1;
}
.search-form ::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #6B6E74 !important;
    opacity:  1;
}
.search-form :-ms-input-placeholder { /* Internet Explorer 10-11 */
    color: #6B6E74 !important;
}

.search-form .btn-search{
    color: #33AFE5;
    display: inline-block;
    font-size: 18px;
    position: absolute;
    right: 12px;
    top: 0px;
    border: 0px;
    background-color: transparent;
}
.search-form .btn-search:hover{ right:13px; }

.table-wrapper table {
    border-collapse: initial !important;
    border-spacing: 0;
}
.table-wrapper table thead > tr > th {
    font-size: 11px;
    font-weight: 600;
    background-color: #38393B;
    color: #ffffff;
    border-radius: 0px;
    border-right: 1px solid rgba(255, 255, 255 ,0.15);
    border-left: 1px solid rgba(0, 0, 0,0.35);
    border-bottom: 0;
    padding: 15px 10px;
}
.table-wrapper table thead > tr > th:first-child { border-left: 0px; border-top-left-radius: 5px; border-bottom-left-radius: 5px;}
.table-wrapper table thead > tr > th:last-child { border-right: 0px; border-top-right-radius: 5px; border-bottom-right-radius: 5px;}

.table-wrapper table thead > tr > th a.datasort{ color: #ffffff; display: block;}
.table-wrapper table thead > tr > th a.datasort > i{ margin-left: 10px;}

.table-wrapper table tbody > tr > td {
    font-size: 12px;
    color: #78797D;
    padding: 2px 10px;
    border-bottom: 1px solid #E5E5E5 !important;
    border-right: 1px solid #E5E5E5 !important;
}
.table-wrapper table tbody > tr > td:first-child { border-left: 1px solid #E5E5E5;}
.table-wrapper table tbody > tr > td td, .table-wrapper table tbody > tr > td td:first-child{ border: 0px;}
.table-wrapper table.striped > tbody > tr:nth-child(2n+1) {  background-color: #FAFAFA;}

.table-wrapper table.actions > tbody > tr > td{ border-right: 0px !important; border-bottom: 0px !important;}
.table-wrapper table.actions td a { font-size: 16px; color: #78797D;}
.table-wrapper table.actions td a:hover, .actions td a:focus { color: #34B0E6;}

.table-input tr td input[type="text"]{ height: 1.5rem; font-size: 1em !important; border-bottom: 0px;}
.table-input tr td input[type="text"]:focus{ border-bottom: 0px; box-shadow: none ;}


/* pie charts and bar charts */

.pie-wrapper .chart-container, .bar-wrapper .chart-container{
    padding: 15px;
    height: 250px !important;
}

.bar-wrapper .chart-container > canvas, .pie-wrapper .chart-container > canvas{
    height: 100% !important;
    width: 100% !important;
}


/* Inner pages styles */

.left-sidebar, .content-wrapper{
    margin: 2.1rem 0 0;
}
.content-wrapper{
    background-color: #ffffff;
    border: 1px solid #dce0e3;
    border-radius: 5px;
    padding: 20px;
}

.two-col-row .col.s2{
    width: 18.6667% !important;
}
.two-col-row .col.s10{
    width: 81.3333% !important;
}
.content-wrapper .page-heading > h1{  margin-top: 20px;}

.side-box .collection {
    border: 0;
    border-radius: 0px;
    margin: 0.5rem 0 1rem;
    overflow: hidden;
    position: relative;
}
.side-box .collection.with-header .collection-header {
    font-size: 0.85em;
    font-weight: 600;
    background-color:#38393B;
    border-bottom: 0;
    padding: 10px 20px;
    color: #ffffff;
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
}
.side-box .collection .collection-item {
    background-color: #fff;
    border-bottom: 1px solid #e0e0e0;
    border-left: 1px solid #e0e0e0;
    border-right: 1px solid #e0e0e0;
    line-height: 1.2rem;
    font-size: 0.75em;
    margin: 0;
    padding: 10px 20px;
    list-style-type:none;
}
.side-box .collection li:last-child {
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
}
.side-box .collection .collection-item > a { display: block; color: #78797d;}
.side-box .collection .collection-item > a:hover { color: #34B0E6;}



/* General form styles */
input[type="text"], input[type="password"], input[type="email"], input[type="url"], input[type="time"], input[type="date"], input[type="datetime-local"], input[type="tel"], input[type="number"], input[type="search"], textarea.materialize-textarea {
    border-color: #dce0e3;
    font-size: 1rem;
    height: 3rem;
    margin: 0 0 0px;
    padding: 0;
    width: 100%;
}
textarea.materialize-textarea { height: 100px !important; padding-top:10px; }
.input-field label {
    color: #34B0E6;
    font-size: 1rem;
}
input[type="text"]:focus:not([readonly]), input[type="password"]:focus:not([readonly]), input[type="email"]:focus:not([readonly]), input[type="url"]:focus:not([readonly]), input[type="time"]:focus:not([readonly]), input[type="date"]:focus:not([readonly]), input[type="datetime-local"]:focus:not([readonly]), input[type="tel"]:focus:not([readonly]), input[type="number"]:focus:not([readonly]), input[type="search"]:focus:not([readonly]), textarea.materialize-textarea:focus:not([readonly]) {
    border-bottom: 1px solid #dce0e3;
    box-shadow: 0 1px 0 0 #dce0e3;
}
input[type="text"]:focus:not([readonly]) + label, input[type="password"]:focus:not([readonly]) + label, input[type="email"]:focus:not([readonly]) + label, input[type="url"]:focus:not([readonly]) + label, input[type="time"]:focus:not([readonly]) + label, input[type="date"]:focus:not([readonly]) + label, input[type="datetime-local"]:focus:not([readonly]) + label, input[type="tel"]:focus:not([readonly]) + label, input[type="number"]:focus:not([readonly]) + label, input[type="search"]:focus:not([readonly]) + label, textarea.materialize-textarea:focus:not([readonly]) + label {
    color: #34B0E6;
}
.select-wrapper input.select-dropdown {
    border-color: #dce0e3;
    display: block;
    font-size: 1rem;
    height: 3rem;
    line-height: 3rem;
    margin: 0 0 15px;
    padding: 0;
    position: relative;
    width: 100%;
}
.browser-default{
    border: 0px;
    -webkit-appearance:none;
    -moz-appearance:none;
    -o-appearance:none;
    -ms-appearance:none;
    appearance:none;
    color: #38393B;
    cursor: pointer;
    font-size: 14px;
    padding: 5px 0 !important;
    border-radius: 0px;
    border-bottom: 1px solid #dce0e3 !important;
}
.selectbox{ position: relative;}
.selectbox .browser-default ~ label{
    position: absolute;
    top: -18px;
    font-size: 0.8rem;
}
/*.selectbox::after{
  content: "";
  display:block;
  background-color: #dce0e3;
  height: 1px;
  width: -moz-calc(100% - 20px);
  width: -webkit-calc(100% - 20px);
  width: -o-calc(100% - 20px);
  width: -ms-calc(100% - 20px);
  width: calc(100% - 20px);
  position: absolute;
  bottom: 0;
}*/
select.browser-default:focus{ outline:0 !important; }
select::-ms-expand { display: none;}

.dropdown-content li > a, .dropdown-content li > span {
    color: #000;
    display: block;
    font-size: 1rem;
    padding: 1rem;
}
.dropdown-content li {
    line-height: 1rem;
}

.browser-default + .icon-arrow{
    position: absolute;
    top: 0;
    right: 12px;
    height: 3rem;
    padding: 8px 5px;
    pointer-events: none;
    color: #34B0E6;
}
.icon-delete{
    cursor: pointer;
    height: 3rem;
    padding: 8px 5px;
    position: absolute;
    top: 0;
    right: 12px;
    color: #34B0E6;
}
.icon-delete:hover, .icon-delete:focus{
    color: #38393B;
}
/* date picker */
.picker__box{ border:0px !important;}
.picker__date-display {
    background-color: #34B0E6;
    color: #fff;
    font-weight: 300;
